{
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "defineExpose": "readonly",
    "withDefaults": "readonly"
  },
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "@vue/typescript/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "import"
  ],
  "rules": {
    "vue/html-self-closing": ["error", {
      "html": {
        "void": "always",
        "normal": "always",
        "component": "always"
      },
      "svg": "always",
      "math": "always"
    }],
    "vue/no-v-html": 0,
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "ts": "never"
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-debugger": "error",
    "vue/max-len": [
      "error",
      {
        "code": 250,
        "template": 250,
        "tabWidth": 2,
        "comments": 250,
        "ignorePattern": "",
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "max-len": [
      "error",
      {
        "code": 250,
        "comments": 250,
        "tabWidth": 2,
        "ignorePattern": "",
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "no-await-in-loop": "error",
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": true
      }
    ],
    "no-self-compare": "error",
    "no-template-curly-in-string": "error",
    "no-use-before-define": [
      "error",
      {
        "functions": true,
        "classes": true,
        "variables": true
      }
    ],
    "camelcase": [
      "error"
    ],
    "complexity": [
      "error",
      {
        "max": 20
      }
    ],
    "eqeqeq": "error",
    "max-depth": [
      "error",
      4
    ],
    "no-empty-function": "error",
    "vue/no-multiple-template-root": "off",
    "no-eval": "error",
    "no-extend-native": "error",
    "no-implicit-coercion": "error",
    "no-implied-eval": "error",
    "no-label-var": "error",
    "no-lone-blocks": "error",
    "no-new": "error",
    "no-new-func": "error",
    "no-new-object": "error",
    "no-new-wrappers": "error",
    "no-script-url": "error",
    "no-underscore-dangle": "error",
    "no-unneeded-ternary": "error",
    "spaced-comment": [
      "error",
      "always"
    ],
    "no-useless-concat": "error",
    "no-useless-rename": [
      "error",
      {
        "ignoreDestructuring": false,
        "ignoreImport": false,
        "ignoreExport": false
      }
    ],
    "no-useless-return": "error",
    "no-var": "error",
    "prefer-const": "error",
    "prefer-object-spread": "error",
    "require-await": "error",
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "arrow-spacing": "error",
    "block-spacing": "error",
    "brace-style": "error",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "computed-property-spacing": [
      "error",
      "never"
    ],
    "func-call-spacing": [
      "error",
      "never"
    ],
    "function-paren-newline": [
      "error",
      "never"
    ],
    "generator-star-spacing": [
      "error",
      {
        "before": true,
        "after": false
      }
    ],
    "implicit-arrow-linebreak": [
      "error",
      "beside"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "max-statements-per-line": [
      "error",
      {
        "max": 1
      }
    ],
    "array-bracket-newline": [
      "error",
      {
        "multiline": true
      }
    ],
    "no-extra-parens": "error",
    "no-multi-spaces": "error",
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxEOF": 1,
        "maxBOF": 1
      }
    ],
    "nonblock-statement-body-position": [
      "error",
      "below"
    ],
    "object-curly-newline": [
      "error",
      {
        "multiline": true
      }
    ],
    "padded-blocks": [
      "error",
      "never"
    ],
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": {
          "max": 5
        },
        "multiline": {
          "max": 1
        }
      }
    ],
    "semi": "off",
    "@typescript-eslint/semi": [
      "error"
    ],
    "no-param-reassign": [
      "error"
    ],
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "vue/attribute-hyphenation": "off",
    "vue/v-on-event-hyphenation": "off",
    "import/no-useless-path-segments": "off",
    "vue/multi-word-component-names": 0
  }
}
